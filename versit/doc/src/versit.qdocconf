project = Qt Versit
description = Qt Versit Reference Documentation

imagedirs = images

headerdirs = ../../ \
            .

sourcedirs = ../../ \
             .

sources.fileextensions  = "*.qdoc *.cpp"

exampledirs = examples/ \
              snippets/ \
              ../../../examples/ \
              ../../../ \
              .

outputdir               = ../html

indexes                 = qt.index

extraimages.HTML        = qtlogo \
                          codeless

versionsym              = "Qt Versit 1.0"

base                    = file:./doc/html

defines                 = Q_QDOC \
                          __cplusplus

